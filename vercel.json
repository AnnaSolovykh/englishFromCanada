{
    "redirects": [
      {
        "source": "/(.*)",
        "has": [
          {
            "type": "host",
            "value": "www.maria-english-incanada.com"
          }
        ],
        "destination": "https://maria-english-incanada.com/$1",
        "permanent": true
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Strict-Transport-Security",
            "value": "max-age=63072000"
          }
        ]
      }
    ]
  }