{
    "redirects": [
      {
        "source": "/:path*",
        "has": [
          {
            "type": "header",
            "key": "x-forwarded-proto",
            "value": "http"
          }
        ],
        "destination": "https://maria-english-incanada.com/:path*",
        "permanent": true,
        "statusCode": 301
      },
      {
        "source": "/:path*",
        "has": [
          {
            "type": "host",
            "value": "www.maria-english-incanada.com"
          }
        ],
        "destination": "https://maria-english-incanada.com/:path*",
        "permanent": true,
        "statusCode": 301
      }
    ]
  }